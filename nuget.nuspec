<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>datatables.net-scroller-se</id>
    <version>2.0.2</version>
    <description>Scroller for DataTables</description>
    <authors>SpryMedia Ltd</authors>
    <projectUrl>http://datatables.net</projectUrl>
    <license type="expression">MIT</license>
    <tags>scroller DataTables jQuery table SemanticUI</tags>
    <repository type="git" url="https://github.com/DataTables/Dist-DataTables-Scroller-SemanticUI.git"/>
    <dependencies>
      <dependency id="jquery" version="1.7.0"/>
      <dependency id="datatables.net" version="1.10.15"/>
      <dependency id="datatables.net-scroller" version="2.0.2"/>
      <dependency id="semanticui" version="2.*"/>
    </dependencies>
    <contentFiles>
      <files include="**/*" buildAction="Content"/>
    </contentFiles>
  </metadata>
  <files>
    <file src="js/scroller.semanticui.js" target="content\Scripts"/>
    <file src="js/scroller.semanticui.min.js" target="content\Scripts"/>
    <file src="css/scroller.semanticui.css" target="content\Content"/>
    <file src="css/scroller.semanticui.min.css" target="content\Content"/>
    <file src="js/scroller.semanticui.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="js/scroller.semanticui.min.js" target="contentFiles\any\any\wwwroot\js"/>
    <file src="css/scroller.semanticui.css" target="contentFiles\any\any\wwwroot\css"/>
    <file src="css/scroller.semanticui.min.css" target="contentFiles\any\any\wwwroot\css"/>
  </files>
</package>
