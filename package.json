{
    "name": "datatables.net-scroller-se",
    "description": "Scroller for DataTables with styling for [SemanticUI](https://semantic-ui.com/)",
    "main": "js/scroller.semanticui.js",
    "module": "js/scroller.semanticui.mjs",
    "style": "css/scroller.semanticui.css",
    "types": "./types/scroller.semanticui.d.ts",
    "version": "2.3.0",
    "files": [
        "css/**/*.css",
        "js/**/*.js",
        "js/**/*.mjs",
        "types/**/*.d.ts"
    ],
    "keywords": [
        "Scroller",
        "Virtual Scrolling",
        "Semantic",
        "SemanticUI",
        "Semantic UI",
        "Datatables",
        "jQuery",
        "table",
        "filter",
        "sort"
    ],
    "dependencies": {
        "datatables.net-scroller": "2.3.0",
        "datatables.net-se": ">=1.11",
        "jquery": ">=1.7"
    },
    "moduleType": [
        "globals",
        "amd",
        "node"
    ],
    "ignore": [
        "composer.json",
        "datatables.json",
        "package.json"
    ],
    "author": {
        "name": "SpryMedia Ltd",
        "url": "http://datatables.net"
    },
    "homepage": "https://datatables.net",
    "bugs": "https://datatables.net/forums",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/DataTables/Dist-DataTables-Scroller-SemanticUI.git"
    }
}